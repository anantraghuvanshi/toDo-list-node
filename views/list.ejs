<%- include("header") -%>

  <div class="box" id="heading">
    <h1><%= kindOfDay %></h1>
  </div>

  <div class="box">

    <% for(var i=0; i<newListItems.length; i++){ %>
    <div class="item">
      <input type="checkbox">
      <p><%= newListItems[i] %></p>
    </div>

    <%  }%>
    <form class="item" action="/" method="post">
      <input placeholder="New Item" type="text" name="newItem" autocomplete="off">
      <button type="submit" name="button">+</button>
    </form>

  </div>
<%- include("footer") -%>
